{
  "name": "particle-network-bg",
  "version": "0.0.4",
  "description": "Interactive particle network animation for backgrounds",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/aliotadi/particle-network-bg.git"
  },
  "homepage": "https://aliotadi.github.io/particle-network-bg/",
  "bugs": {
    "url": "https://github.com/aliotadi/particle-network-bg/issues"
  },
  "keywords": [
    "particle",
    "canvas",
    "animation",
    "background",
    "network",
    "react"
  ],
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./react": {
      "types": "./dist/react.d.ts",
      "import": "./dist/react.mjs",
      "require": "./dist/react.js"
    }
  },
  "peerDependencies": {
    "react": ">=18.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@types/react": "^19.2.14",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  }
}
